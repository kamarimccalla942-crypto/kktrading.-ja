<?xml version="1.0" encoding="UTF-8"?>
<xml xmlns="http://www.w3.org/1999/xhtml" is_dbot="true" collection="false">
  
  <!-- VARIABLE DEFINITIONS -->
  <variables>
    <!-- Core Trading Variables -->
    <variable type="" id="var_name">NAME</variable>
    <variable type="" id="var_base_stake">Base_Stake</variable>
    <variable type="" id="var_current_stake">Current_Stake</variable>
    <variable type="" id="var_loss_count">Loss_Count</variable>
    <variable type="" id="var_win_count">Win_Count</variable>
    <variable type="" id="var_consecutive_losses">Consecutive_Losses</variable>
    
    <!-- Advanced Analysis Variables -->
    <variable type="" id="var_trend_list">Trend_List</variable>
    <variable type="" id="var_digit_analysis">Digit_Analysis</variable>
    <variable type="" id="var_volatility_score">Volatility_Score</variable>
    <variable type="" id="var_confidence_level">Confidence_Level</variable>
    <variable type="" id="var_market_momentum">Market_Momentum</variable>
    <variable type="" id="var_resistance_levels">Resistance_Levels</variable>
    <variable type="" id="var_support_levels">Support_Levels</variable>
    
    <!-- Risk Management Variables -->
    <variable type="" id="var_stop_loss">Stop_Loss</variable>
    <variable type="" id="var_expected_profit">Expected_Profit</variable>
    <variable type="" id="var_daily_limit">Daily_Limit</variable>
    <variable type="" id="var_session_profit">Session_Profit</variable>
    <variable type="" id="var_max_loss_level">Max_Loss_Level</variable>
    <variable type="" id="var_position_size">Position_Size</variable>
    <variable type="" id="var_risk_reward_ratio">Risk_Reward_Ratio</variable>
    
    <!-- Prediction & Pattern Variables -->
    <variable type="" id="var_digit_frequency">Digit_Frequency</variable>
    <variable type="" id="var_pattern_detected">Pattern_Detected</variable>
    <variable type="" id="var_historical_data">Historical_Data</variable>
    <variable type="" id="var_prediction_score">Prediction_Score</variable>
    <variable type="" id="var_signal_strength">Signal_Strength</variable>
    
    <!-- Display Variables -->
    <variable type="" id="var_display_status">Display_Status</variable>
    <variable type="" id="var_analysis_text">Analysis_Text</variable>
    <variable type="" id="var_performance_text">Performance_Text</variable>
    <variable type="" id="var_notification_msg">Notification_Msg</variable>
  </variables>

  <!-- MAIN TRADE DEFINITION BLOCK -->
  <block type="trade_definition" id="main_trade_def" deletable="false" x="0" y="0">
    
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" id="market_config" deletable="false" movable="false">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">1HZ10V</field>
        
        <next>
          <block type="trade_definition_tradetype" id="tradetype_config" deletable="false" movable="false">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">overunder</field>
            
            <next>
              <block type="trade_definition_contracttype" id="contract_config" deletable="false" movable="false">
                <field name="TYPE_LIST">DIGITUNDER</field>
                
                <next>
                  <block type="trade_definition_candleinterval" id="candle_config" deletable="false" movable="false">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    
                    <next>
                      <block type="trade_definition_restartbuysell" id="restart_config" deletable="false" movable="false">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        
                        <next>
                          <block type="trade_definition_restartonerror" id="error_config" deletable="false" movable="false">
                            <field name="RESTARTONERROR">TRUE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>

    <!-- INITIALIZATION BLOCK -->
    <statement name="INITIALIZATION">
      <block type="text_print" id="init_header" collapsed="false">
        <value name="TEXT">
          <shadow type="text" id="init_text">
